{
  "build": {
    "beforeDevCommand": "vite",
    "beforeBuildCommand": "vite build",
    "beforeBundleCommand": "node copy-sidecar.cjs choreo-cli ./target",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": [
      "appimage",
      "deb",
      "dmg",
      "nsis",
      "rpm"
    ],
    "macOS": {
      "minimumSystemVersion": "14.5",
      "signingIdentity": "-"
    },
    "shortDescription": "Choreo",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "../target/choreo-cli"
    ]
  },
  "productName": "Choreo",
  "version": "2026.0.0-beta-1",
  "identifier": "org.sleipnirgroup",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Choreo v2026.0.0-beta-1",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "capabilities": [
        "main-capability"
      ],
      "csp": null
    }
  }
}
